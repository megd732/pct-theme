<?php get_header(); ?>
  <section class="content">
    <div class="container">
      <div class="row">
        <?php
        if ( have_posts() ) : while ( have_posts() ) : the_post();

	  get_template_part( 'content', get_post_format() ); 
        endwhile; endif;
        ?>
      </div>
    </div>

  </section>

<?php get_footer(); ?>

